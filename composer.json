{
  "name": "matthiasmullie/scrapbook",
  "type": "library",
  "description": "A standard cache interface with adapters for a lot of key-value stores, along with some transactional capabilities.",
  "keywords": [
    "cache", "caching",
    "key", "value", "store",
    "apc",
    "memcached",
    "redis",
    "couchbase",
    "sql", "mysql", "sqlite", "postgresql",
    "filesystem",
    "buffer", "buffered",
    "transaction", "transactional", "commit", "rollback",
    "psr6", "psr-6", "psr-cache"
  ],
  "homepage": "http://scrapbook.cash",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthias Mullie",
      "homepage": "http://www.mullie.eu",
      "email": "scrapbook@mullie.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*",
    "satooshi/php-coveralls": "dev-master",
    "apigen/apigen": "~4.0"
  },
  "suggest": {
    "ext-apc": ">=3.1.1",
    "ext-couchbase": ">=2.0.0",
    "ext-memcached": ">=2.0.0",
    "ext-redis": ">=2.2.0 || 0.0.0.0",
    "ext-pdo": ">=0.1.0"
  },
  "provide": {
    "psr/cache-implementation": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "MatthiasMullie\\Scrapbook\\": "src/",
      "MatthiasMullie\\Scrapbook\\Tests\\": "tests/",
      "Psr\\Cache\\": "src/Psr6/Psr/"
    }
  }
}
